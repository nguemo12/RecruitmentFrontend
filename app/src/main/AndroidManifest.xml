<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.go_jobs" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/work"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Go_Jobs"
        android:usesCleartextTraffic="true" >
        <activity
            android:name=".CategoryList"
            android:exported="false" />
        <activity
            android:name=".NewCategory"
            android:exported="false"
            android:label="New Category" />
        <activity
            android:name=".ApplicationList"
            android:exported="false" />
        <activity
            android:name=".ApplicantList"
            android:exported="false" />
        <activity
            android:name=".interview"
            android:exported="false" />
        <activity
            android:name=".NewApplicant"
            android:exported="false"
            android:label="Applicant Register" />
        <activity
            android:name=".NewCompany"
            android:exported="false"
            android:label="Company Register" />
        <activity
            android:name=".PostJob"
            android:exported="false"
            android:label="Add new Offer" />
        <activity
            android:name=".jobs"
            android:exported="false" />
        <activity
            android:name=".Job_Apply"
            android:exported="false"
            android:label="Application Form" />
        <activity
            android:name=".job_description"
            android:exported="false" />
        <activity
            android:name=".setting"
            android:exported="false" />
        <activity
            android:name=".Message"
            android:exported="false" />
        <activity
            android:name=".Register"
            android:exported="false"
            android:label="Register"
            android:parentActivityName=".Login" />
        <activity
            android:name=".Home_screen"
            android:exported="false"
            android:parentActivityName=".Login" />
        <activity
            android:name=".Login"
            android:exported="false"
            android:label="Login Page"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MainActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>